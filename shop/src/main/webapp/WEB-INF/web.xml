<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
	<display-name>Archetype Created Web Application</display-name>


	<servlet>
		<servlet-name>Contact</servlet-name>
		<display-name>Contact</display-name>
		<description></description>
		<servlet-class>com.petrolhead.servlet.Contact</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>Login</servlet-name>
		<display-name>Login</display-name>
		<description></description>
		<servlet-class>com.petrolhead.servlet.Login</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>Signup</servlet-name>
		<display-name>Signup</display-name>
		<description></description>
		<servlet-class>com.petrolhead.servlet.Signup</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Contact</servlet-name>
		<url-pattern>/Contact</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>Login</servlet-name>
		<url-pattern>/Login</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>Signup</servlet-name>
		<url-pattern>/Signup</url-pattern>
	</servlet-mapping>

	<welcome-file-list>
		<welcome-file>masterpage.jsp</welcome-file>
	</welcome-file-list>
</web-app>
